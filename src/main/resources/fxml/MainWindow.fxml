<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="458.0" prefWidth="320" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.suai.paintclient.MainWindowController">
   <children>
      <VBox layoutX="-2.0" prefHeight="458.0" prefWidth="320.0">
         <children>
            <HBox>
               <children>
                  <VBox>
                     <children>
                        <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="348.0" onAction="#UpdateList" text="Update list of rooms" />
                        <ListView fx:id="list_of_view" prefHeight="708.0" prefWidth="320.0" />
                        <Button fx:id="btnCreate" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#createRoom" prefHeight="26.0" prefWidth="436.0" text="Создать новую" textAlignment="RIGHT">
                           <opaqueInsets>
                              <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
                           </opaqueInsets>
                        </Button>
                        <Button fx:id="btnJoin" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#join" prefHeight="26.0" prefWidth="542.0" text="Присоединиться" textAlignment="CENTER">
                           <opaqueInsets>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </opaqueInsets>
                        </Button>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
